{
  "name": "abax-node-sdk",
  "version": "3.2.3",
  "description": "SDK for Abax API",
  "license": "Apache-2.0",
  "repository": "github:bjerkio/abax-node-sdk",
  "author": "Bjerk AS <office@bjerk.io> (https://bjerk.io)",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/main.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest **/*.test.ts",
    "test:all": "TZ=UTC vitest",
    "test:e2e": "TZ=UTC vitest **/*.spec.ts",
    "test:ui": "TZ=UTC vitest --ui"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "exponential-backoff": "^3.1.1",
    "fast-jwt": "^3.1.1",
    "ts-invariant": "^0.10.3",
    "typical-fetch": "^2.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@bjerk/eslint-config": "^5.3.1",
    "@simenandre/prettier": "^5.0.0",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node-lts": "^18.12.2",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^18",
    "eslint": "^8.45.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "undici": "^5.28.2",
    "vitest": "^0.34.0"
  },
  "keywords": [
    "abax",
    "abax-api",
    "node",
    "sdk"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": "./dist/main.js",
  "packageManager": "pnpm@8.12.1",
  "prettier": "@simenandre/prettier",
  "volta": {
    "node": "18.17.0"
  }
}
