{
  "name": "abax-node-sdk",
  "version": "1.1.2",
  "description": "SDK for Abax API",
  "license": "Apache-2.0",
  "author": "Bjerk AS <office@bjerk.io> (https://bjerk.io)",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/main.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "TZ=UTC vitest"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "exponential-backoff": "^3.1.1",
    "typical-fetch": "^2.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@bjerk/eslint-config": "^4.1.0",
    "@simenandre/prettier": "^4.0.1",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node-lts": "^18.12.2",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^18",
    "eslint": "^8.43.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "undici": "^5.22.1",
    "vitest": "^0.32.2"
  },
  "keywords": [
    "abax",
    "abax-api",
    "node",
    "sdk"
  ],
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": "@bjerk/eslint-config",
    "ignorePatterns": "dist/"
  },
  "exports": "./dist/main.js",
  "packageManager": "pnpm@8.6.5",
  "prettier": "@simenandre/prettier",
  "volta": {
    "node": "18.16.1"
  }
